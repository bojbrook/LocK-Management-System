#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>
#include <stddef.h>
#include <string.h>


#define CACHESIZE               16





typedef struct Cache{
    char *cruzID = "unknown ID"; //10 characters + null terminator
    unsigned int second: 8;
    unsigned int minute: 8;
    unsigned int hour: 4;
    unsigned int day: 5;
    unsigned int month: 4;
}Cache;


Cache *CacheInit();
void CacheDelete(Cache *cache);
Cache *CacheClear(Cache *cache);

